BEGIN 
FIM: EXTERN 
COPY ZERO,TEMP 
INPUT FINAL 
LOAD FINAL 
JMPZ RAIZ 
INIC: LOAD TEMP 
TESTANDO: ADD ONE 
STORE TEMP 
MUL TEMP 
SUB FINAL 
JMPN ERRO 
JMPP INIC 
RAIZ: OUTPUT TEMP 
JMP FIM 
ERRO: OUTPUT MINUS 
JMP FIM 
COPY ONE,FIM 
MINUS: CONST -1 
ZERO: CONST 0 
ONE: CONST 1 
FINAL: SPACE 
TEMP: SPACE 
END 
